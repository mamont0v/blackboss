{
  "name": "blackboss",
  "version": "1.0.0",
  "description": "shop clother",
  "main": "server.js",
  "type": "module",
  "engines": {
    "node": "14.15.0",
    "npm": "6.14.8"
  },
  "scripts": {
    "heroku-postbuild-new": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "client": "cd client && npm start",
    "server": "nodemon backend/server",
    "build": "cd client && npm run build",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" ",
    "NEED DOBAVIT v dev": " \"npm run client\" ",
    "start": "node backend/server.js",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build",
    "data:import": "node seeder",
    "data:destroy": "node seeder -d"
  },
  "author": "Valentin Mamontov",
  "license": "ISC",
  "dependencies": {
  },
  "devDependencies": {
  }
}
